<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DjSignUp">

    <EditText
        android:id="@+id/et_v_dsu_pass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="7dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/et_v_dsu_dName" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="22dp"
        android:text="Password:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_v_dsu_pass"
        app:layout_constraintEnd_toStartOf="@+id/et_v_dsu_pass"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="31dp"
        android:text="DJ Sign Up"
        app:layout_constraintBottom_toTopOf="@+id/et_v_dsu_dName"
        app:layout_constraintStart_toStartOf="@+id/et_v_dsu_dName" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="27dp"
        android:text="DJ Name:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_v_dsu_dName"
        app:layout_constraintEnd_toStartOf="@+id/et_v_dsu_dName"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_v_dsu_dName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="153dp"
        android:layout_marginEnd="7dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_v_dsu_signUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="92dp"
        android:text="Sign Up"
        app:layout_constraintStart_toStartOf="@+id/et_v_dsu_pass"
        app:layout_constraintTop_toBottomOf="@+id/et_v_dsu_pass" />

    <Button
        android:id="@+id/btn_v_dsu_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginTop="25dp"
        android:text="Back"
        app:layout_constraintStart_toStartOf="@+id/btn_v_dsu_signUp"
        app:layout_constraintTop_toBottomOf="@+id/btn_v_dsu_signUp" />

</androidx.constraintlayout.widget.ConstraintLayout>