<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".EventInfo" >

    <ImageView
        android:id="@+id/imageView97"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle_1" />

    <ImageView
        android:id="@+id/imageView98"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView97"
        app:srcCompat="@drawable/code__date__location__time__private_" />

    <ImageView
        android:id="@+id/imageView99"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97"
        app:srcCompat="@drawable/rectangle_14" />

    <ImageView
        android:id="@+id/imageView100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="94dp"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97"
        app:srcCompat="@drawable/rectangle_14" />

    <ImageView
        android:id="@+id/imageView101"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="143dp"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97"
        app:srcCompat="@drawable/rectangle_14" />

    <ImageView
        android:id="@+id/imageView102"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="189dp"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97"
        app:srcCompat="@drawable/rectangle_14" />

    <ImageButton
        android:id="@+id/btn_v_ei_privateN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="129dp"
        android:layout_marginTop="10dp"
        android:background="@android:drawable/divider_horizontal_bright"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView102"
        app:srcCompat="@drawable/rectangle_31" />

    <ImageButton
        android:id="@+id/btn_v_ei_privateY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:background="@android:drawable/divider_horizontal_bright"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView102"
        app:srcCompat="@drawable/rectangle_31" />

    <EditText
        android:id="@+id/et_v_ei_time"
        android:layout_width="182dp"
        android:layout_height="46dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="189dp"
        android:ems="10"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97" />

    <EditText
        android:id="@+id/et_v_ei_location"
        android:layout_width="182dp"
        android:layout_height="46dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="143dp"
        android:ems="10"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97" />

    <EditText
        android:id="@+id/et_v_ei_date"
        android:layout_width="182dp"
        android:layout_height="46dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="94dp"
        android:ems="10"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="64dp"
        android:text="Event Information:"
        android:textColor="#FAFAFA"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_v_ei_privateY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="239dp"
        android:text="YES"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97" />

    <TextView
        android:id="@+id/tv_v_ei_priavteN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="150dp"
        android:layout_marginTop="239dp"
        android:text="NO"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97" />

    <ImageButton
        android:id="@+id/btn_v_ei_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="224dp"
        android:background="@android:drawable/divider_horizontal_bright"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/rectangle_29" />

    <ImageButton
        android:id="@+id/btn_v_ei_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="164dp"
        android:background="@android:drawable/divider_horizontal_bright"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/rectangle_29" />

    <TextView
        android:id="@+id/tv_v_ei_code"
        android:layout_width="177dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="48dp"
        android:text="TextView"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView97" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginBottom="172dp"
        android:text="Delete Event"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginBottom="232dp"
        android:text="Update"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>